import java.util.Scanner;
class Main {
  public static void main(String[] args) {

    int i,j,n;
    
  System.out.print ("insira o numero");
  Scanner scan = new Scanner (System.in);
    n = scan.nextInt();

  for (i=0;i<=n;i++){
    for (j=0;j<=i;j++)
    System.out.print(j);
    System.out.println("");
  }
}
    }
